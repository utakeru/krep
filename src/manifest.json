{
  "name": "krep",
  "version": "0.0.12",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://*.cybozu.com/*",
        "https://*.cybozu-dev.com/*"
      ],
      "js": [
        "main.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "krep-icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
